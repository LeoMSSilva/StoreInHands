{
	"expo": {
		"name": "storeInHands",
		"slug": "storeInHands",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "cover",
			"myBackgroundColor": "#e6f0ff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.leomssilva.storeInHands"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"myBackgroundColor": "#e6f0ff"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}
